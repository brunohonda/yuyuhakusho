{
    "env": {
        "es2020": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "comma-dangle": [ "error", "never" ],
        "object-curly-spacing": [ "error", "always" ]
    }
}
